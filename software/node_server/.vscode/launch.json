    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js"
        },
        {
            "address": "rocketlogger",
            "localRoot": "${workspaceFolder}",
            "name": "RocketLogger",
            "port": 9229,
            "remoteRoot": "/home/rocketlogger/node_server",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "pwa-node"
        }
    ]
}